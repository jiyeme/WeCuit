@media (prefers-color-scheme: dark) {
    .scroll__loading {
        color: rgb(99, 99, 99);
    }

    .success {
        color: rgb(38, 117, 238);
    }

    .success:after {
        background-color: rgb(0, 0, 0);
    }

    .success > .info {
        color: rgb(17, 17, 17);
    }

    .empty__text {
        color: rgb(99, 99, 99);
    }

    .arrow {
        background: rgb(88, 88, 88);
    }

    .arrow:before {
        background: rgb(88, 88, 88);
    }

    .arrow:after {
        background: rgb(88, 88, 88);
    }

    .loading__item:before {
        background-color: rgb(88, 88, 88);
    }

    @keyframes fadeDelay {
        0%, 39%, 100% {
            opacity: 0.2;
        }

        40% {
            opacity: 1;
        }
    }
}
